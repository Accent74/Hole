                    <xs:element type="xs:string" name="VIEW_DOC"/>
                    
                      <xs:element name="DATE_DOC">
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:pattern value="((0[1-9]|[12][0-9]|3[01])\.(0[1-9]|1[012])\.(19|20)\d\d)*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    
                    <xs:element type="xs:string" name="N_DOC"/>
                    <xs:element type="xs:float" name="SUMMA"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="NOT_INCLUDED_INCOMES">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ROW" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:long" name="NPP"/>
                    <xs:element type="xs:string" name="INN"/>
                    <xs:element type="xs:string" name="NAME"/>
                    <xs:element type="xs:float" name="SUMMA"/>
                    <xs:element type="xs:string" name="STRING_DECL"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="NOT_INCLUDED_COSTS">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ROW" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:long" name="NPP"/>
                    <xs:element type="xs:string" name="INN"/>
                    <xs:element type="xs:string" name="NAME"/>
                    <xs:element type="xs:float" name="SUMMA"/>
                    <xs:element type="xs:string" name="STRING_DECL"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="ATTACHMENT_AM">
          <xs:complexType>
            <xs:sequence> 
              <xs:element name="ROW" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                   <xs:element type="xs:long" name="NPP"/>
                    <xs:element type="xs:string" name="N_GROUP"/>
                    <xs:element type="xs:float" name="PRICE"/>
                    <xs:element type="xs:float" name="BEGIN_PERIOD"/>
                    <xs:element type="xs:float" name="END_PERIOD"/>
                    <xs:element type="xs:float" name="SUMMA"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>